<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/felixmoon"
    android:backgroundTint="#C6030101"
    android:backgroundTintMode="src_atop"
    tools:context=".Series">


    <androidx.viewpager.widget.ViewPager
        android:layout_width="match_parent"
        android:layout_height="fill_parent"
        android:id="@+id/pager"
       >

        <com.google.android.material.tabs.TabLayout android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:id="@+id/tablayout"
            app:tabSelectedTextColor="#BF360C"
            android:background="@drawable/falkirk"
            android:backgroundTint="#DF140404"
            android:backgroundTintMode="src_atop"/>


    </androidx.viewpager.widget.ViewPager>


</FrameLayout>